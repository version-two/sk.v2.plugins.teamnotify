<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>team-notify</name>
    <display-name>TeamNotify</display-name>
    <version>@Version@</version>
    <description>Sends build notifications to various services via webhooks.</description>
    <vendor>
      <name>Version Two s.r.o.</name>
      <url>https://versiontwo.sk</url>
    </vendor>
  </info>
  <deployment use-separate-classloader="false" />
</teamcity-plugin>
